        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jean Jar Pereira de Ara√∫jo",
            "email": "jeanjpa@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "JeanJar\\LaravelHelpers\\Http\\Providers\\FunctionServiceProvider"
            ]
        }
    }
}

{
    "name": "jeanjar/simple-helper-functions-for-laravel",
    "description": "Helper functions for laravel 5",
    "require": {
        "php" : "^7.2",
        "illuminate/http": "^5.5",
        "illuminate/routing": "^5.5",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
